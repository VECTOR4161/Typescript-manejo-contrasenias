// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Servicio {
  id          Int     @id @default(autoincrement())
  servicio    String
  usuario     String
  contrasenia String
  borrado     Boolean @default(false)
}
